<keymap version="1" name="Default copy" parent="$default">
  <action id="EditorDown">
    <keyboard-shortcut first-keystroke="down" />
    <keyboard-shortcut first-keystroke="ctrl n" />
  </action>
  <action id="EditorUp">
    <keyboard-shortcut first-keystroke="up" />
    <keyboard-shortcut first-keystroke="ctrl p" />
  </action>
  <action id="EmmetNextEditPoint" />
  <action id="EmmetPreviousEditPoint" />
  <action id="GotoClass">
    <keyboard-shortcut first-keystroke="ctrl alt n" />
  </action>
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="alt right" />
    <keyboard-shortcut first-keystroke="shift alt close_bracket" />
  </action>
  <action id="ParameterInfo">
    <keyboard-shortcut first-keystroke="alt p" />
  </action>
  <action id="PreviousTab">
    <keyboard-shortcut first-keystroke="alt left" />
    <keyboard-shortcut first-keystroke="shift alt open_bracket" />
  </action>
</keymap>